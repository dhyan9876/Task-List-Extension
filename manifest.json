{
  "manifest_version": 3,
  "name": "Task List - Focus Mode",
  "version": "1.0",
  "description": "A simple task list built for focus mode",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}