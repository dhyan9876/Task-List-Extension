{
  "manifest_version": 3,
  "name": "One Task Only - To Do and Task List",
  "version": "1.0",
  "description": "One Task at a Time!",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}